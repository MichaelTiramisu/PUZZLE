<?xml version='1.0' encoding='utf-8'?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0" metadata-complete="true">

    
    <servlet>
        <servlet-name>GetNewsSevrlet</servlet-name>
        <servlet-class>edu.missouri.cs.news.api.get_news.GetNewsSevrlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetNewsSevrlet</servlet-name>
        <url-pattern>/api/authentication/get_all_news</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>GetUsers</servlet-name>
        <servlet-class>edu.missouri.cs.news.api.get_news.GetUsers</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetUsers</servlet-name>
        <url-pattern>/api/authentication/get_users</url-pattern>
    </servlet-mapping>
    
    
</web-app>
